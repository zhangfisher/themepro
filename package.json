{
	"name": "themepro",
	"module": "dist/index.js",
	"type": "module",
	"version": "1.0.7",
	"repository": {
		"type": "git",
		"url": "https://github.com/zhangfisher/themepro.git"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.umd.js"
		},
		"./index.css": {
			"import": "./dist/index.css",
			"require": "./dist/index.css"
		},
		"./themes/*.css": {
			"import": "./dist/*.css",
			"require": "./dist/*.css"
		}
	},
	"files": [
		"dist",
		"readme.md"
	],
	"scripts": {
		"dev": "vite",
		"storybook": "storybook dev -p 6006",
		"build:storybook": "storybook build",
		"build": "bun run build:css && tsup",
		"build:tsup": "tsup",
		"docs:dev": "vitepress dev docs",
		"docs:build": "vitepress build docs",
		"docs:preview": "vitepress preview docs",
		"changeset": "changeset",
		"version": "changeset version",
		"release": "bun run build && changeset version && changeset publish"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.2.4",
		"@changesets/cli": "^2.29.7",
		"@lite-tree/vue": "^1.1.6",
		"@storybook/addon-a11y": "^9.1.10",
		"@storybook/web-components-vite": "^9.1.10",
		"@types/bun": "1.2.22",
		"biome": "^0.3.3",
		"cpx": "^1.5.0",
		"esbuild-copy-files-plugin": "^1.2.1",
		"less": "^4.4.1",
		"lit": "^3.3.1",
		"storybook": "^9.1.10",
		"tsup": "^8.5.0",
		"typescript": "^5.8.2",
		"unplugin-dts": "1.0.0-beta.6",
		"vanilla-picker": "^2.12.3",
		"vite": "^7.1.7",
		"vite-plugin-cp": "^6.0.3",
		"vitepress": "^1.6.4",
		"vitepress-demo-plugin": "^1.4.7"
	},
	"peerDependencies": {
		"typescript": "^5"
	},
	"dependencies": {
		"@ant-design/colors": "^7.2.1",
		"@ant-design/fast-color": "^3.0.0"
	},
	"packages": [
		"packages/*"
	],
	"packageManager": "bun@1.2.23"
}
